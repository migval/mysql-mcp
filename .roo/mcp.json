{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "alwaysAllow": ["resolve-library-id"]
    },
    "mysql": {
      "command": "node",
      "args": ["src/index.js"],
      "env": {
        "DB_HOST": "localhost",
        "DB_PORT": "3306",
        "DB_USER": "root",
        "DB_PASSWORD": "root",
        "DB_NAME": "dictea"
      }
    }
  }
}
